msgid "UPDATE VERSION:"
msgstr "Verzia aktualizácie:"

#: 
msgid "UPDATE CHANGELOG:"
msgstr "Zoznam zmien aktualizácie:"

#: 
msgid "MORE DETAILS"
msgstr "Viac podrobností"

#: 
msgid "CAROUSEL ANIMATION"
msgstr "Otočná animácia"

#: 
msgid "THEME CONFIGURATION"
msgstr "Nastavenie témy"

#: 
msgid "THEME COLORSET"
msgstr "Farebná sada témy"

#: 
msgid "THEME ICONSET"
msgstr "Sada ikon témy"

